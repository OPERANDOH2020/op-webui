    // Function call when clicked "Ok" of one row
    function okRowClick(idRiga, bottone, report) {
        // OSP
        var valOsps = [];
        $('#editOSP_' + idRiga +' :selected').each(function (i, selected) {
            valOsps[i] = $(selected).text().trim();
        });

        // Start date
        var startDate = $('#editStartDate_' + idRiga).val();

        // Run Every
        var repeatEveryNumb = $('#editRepeatNumb_' + idRiga).val();
        var repeatEveryType = $('#editRepeatType_' + idRiga + '-interval-selection option:selected').text();

        // On
        var valDayOfMonth = "";
        var valDayOfYear = "";
        var valDayOfWeek = "";

        if (repeatEveryType.toUpperCase() == "WEEK(S)") // WEEKS
            valDayOfWeek = $('#editRepeatType_' + idRiga + '-reportb-days-list option:selected').map(function (i, n) {
                return n.text;
            }).get().join(",");
        else
            if (repeatEveryType.toUpperCase() == "MONTH(S)") // MONTHS
                valDayOfMonth = $('#editRepeatType_' + idRiga + '-reportc-numtday').val();
            else
                if (repeatEveryType.toUpperCase() == "YEAR(S)") // YEARS
                    valDayOfYear = $('#editRepeatType_' + idRiga + '-reportd-date').val();

        // Storage period
        var valStoragePeriodNumb = $('#editStorageEveryNumb_'+idRiga).val();
        var valStoragePeriodType = $('#editStorageEveryType_' + idRiga + ' :selected').val();            

        // Qua ci metti la chiamata ajax che fa la modifica
        $.ajax({
            async: false,
            type: "POST",
            url: "../ASPXSaveAjax/SaveSchedule.aspx",
            data: {
                ID: idRiga,
                Report: report,
                StartDate: startDate,
                RepeatEveryNumb: repeatEveryNumb,
                RepeatEveryType: repeatEveryType,
                StoragePeriodNumb: valStoragePeriodNumb,
                StoragePeriodType: valStoragePeriodType,
                OSPs: valOsps.join(),
                DayOfWeek: valDayOfWeek,
                Time: '',
                DayOfMonth: valDayOfMonth,
                DayOfYear: valDayOfYear
            },
            dataType: "text",
            success: function (data) {
                // Nascondo gli input di inserimento e ripristino le label
                $('.scheduleDetail_' + idRiga).toggle();

                // Qua ci metti le modifiche alle label
                $('#labelOSP_' + idRiga).text(valOsps.join(', '));
                $('#labelStartDate_' + idRiga).text(startDate);
                $('#labelRepeatEveryNumb_' + idRiga).text(repeatEveryNumb);
                $('#labelRepeatEveryType_' + idRiga).text(repeatEveryType);

                if (repeatEveryType.toUpperCase() == "DAY(S)") // DAYS
                    $('#labelOn_' + idRiga).text('-');
                else
                    if (repeatEveryType.toUpperCase() == "WEEK(S)") // WEEKS
                        $('#labelOn_' + idRiga).text(valDayOfWeek.replace(/,/g,' '));
                    else
                        if (repeatEveryType.toUpperCase() == "MONTH(S)") // MONTHS
                            $('#labelOn_' + idRiga).text(valDayOfMonth);
                        else
                            if (repeatEveryType.toUpperCase() == "YEAR(S)") // YEARS
                                $('#labelOn_' + idRiga).text(valDayOfYear);

                $('#labelStorageEveryNumb_' + idRiga).text(valStoragePeriodNumb);
                $('#labelStorageEveryType_' + idRiga).text(valStoragePeriodType);
            },
            error: function (xhr, status) {
                alert("Error: " + xhr.statusText);
            }
        });
    }

