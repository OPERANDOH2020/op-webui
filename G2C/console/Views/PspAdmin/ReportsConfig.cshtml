                                                                                <th>Run every</th>
                                                                                <th>On</th>
                                                                                <th>Storage period</th>
                                                                                <th style="width: 125px; min-width: 125px">Actions</th>
                                                                            </tr>
                                                                        </thead>

                                                                        @* ------------------------------------------------------------[ INIZIO DETTAGLIO SCHEDULAZIONE ] ------------------------------------------------------------------------*@
                                                                        <tbody>
                                                                            @foreach (var details in Model.schedulesObj.ScheduleDetailsList)
                                                                            {
                                                                                if (details.Report == item.Report)
