                                                                                                @foreach (var tmp in @item.RepeatEveryTypeOption)
                                                                                                {
                                                                                                    @Html.Raw(tmp)
                                                                                                }
                                                                                            </select>
                                                                                        </div>
                                                                                    </div>
                                                                                </td>

                                                                                @* ---[ ON ]---*@
                                                                                <td>
                                                                                    <div class="input-group" id="@item.Report.Replace(" ","_").Replace("'","-")-Reporta" style="width: 100px">
                                                                                        -
                                                                                    </div>
                                                                                    <div class="input-group" id="@item.Report.Replace(" ","_").Replace("'","-")-Reportb" style="display:none;">
                                                                                        <select id="@item.Report.Replace(" ","_").Replace("'","-")-reportb-days-list" data-width="100px" class="bs-select form-control" multiple>
                                                                                            @foreach (var tmp in @item.DayOfWeekOption)
                                                                                            {
                                                                                                @Html.Raw(tmp)
                                                                                            }
                                                                                        </select>
                                                                                    </div>
                                                                                    <div class="input-group" id="@item.Report.Replace(" ","_").Replace("'","-")-Reportc" style="display:none">
                                                                                        <input id="@item.Report.Replace(" ","_").Replace("'","-")-reportc-numtday" style="width:39px; text-align:center" class="positive touchspin_days_in_month" type="text" value="0" name="number">
