                                                                                    <tr id="row_@details.ID@Html.Raw("-")@foreach (var tmp in details.OSPs)
                                                                                    {
                                                                                        @Html.Raw(tmp + "-")
                                                                                    }">
                                                                                        @* ---[ OSP ]---*@
                                                                                        <td>
                                                                                            <div class="scheduleDetail_@details.ID">
                                                                                                <label id="labelOSP_@details.ID">@Html.Raw(string.Join(", ", details.OSPs))</label>
                                                                                            </div>
                                                                                            @* ---[ edit ]---*@
                                                                                            <div style="display:none" class="input-group scheduleDetail_@details.ID">
                                                                                                <select id="editOSP_@details.ID" data-width="60px" class="bs-select form-control" multiple>
                                                                                                    @foreach (var tmp in Model.reportsObj.ReportList[0].OSPs)
                                                                                                    {
                                                                                                        <option value='@Html.Raw(tmp)' @if (details.OSPs.Contains(tmp.ToString())) { @Html.Raw("selected") }> @Html.Raw(tmp)</option>
                                                                                                    }
                                                                                                </select>
                                                                                            </div>

                                                                                        </td>

                                                                                        @* ---[ START DATE ]---*@
                                                                                        <td>
                                                                                            <div class="scheduleDetail_@details.ID">
                                                                                                <label id="labelStartDate_@details.ID">
                                                                                                    @details.StartDate.ToString("dd/MM/yyyy hh:mm")
                                                                                                </label>
                                                                                            </div>
                                                                                            @* ---[ edit ]---*@
                                                                                            <div style="display:none" class="input-group scheduleDetail_@details.ID">
                                                                                                <div class="input-group">
                                                                                                    <input id="editStartDate_@details.ID" value="@details.StartDate.ToString("dd/MM/yyyy hh:mm")" type="text" class="form-control form_datetime" readonly name="editStartDate_@details.ID" />
                                                                                                    <span class="input-group-btn">
                                                                                                        <button class="btn default" type="button" onclick="$('#editStartDate_@details.ID').datetimepicker('show')">
                                                                                                            <i class="fa fa-calendar"></i>
                                                                                                        </button>
                                                                                                    </span>
                                                                                                </div>
                                                                                            </div>
                                                                                        </td>

                                                                                        @* ---[ RUN EVERY ]---*@
                                                                                        <td>
                                                                                            <div class="scheduleDetail_@details.ID">
                                                                                                <label id="labelRepeatEveryNumb_@details.ID"> @details.RepeatEveryNumb </label>  -
                                                                                                <label id="labelRepeatEveryType_@details.ID"> @details.RepeatEveryType </label>
                                                                                            </div>
                                                                                            @* ---[ edit ]---*@
                                                                                            <div style="display:none" class="input-group scheduleDetail_@details.ID">
                                                                                                <div class="col-md-7">
                                                                                                    <input style="width:57px; text-align:center" id="editRepeatNumb_@details.ID" class="positive touchspin_default" type="text" value="@details.RepeatEveryNumb" name="number">
                                                                                                </div>
                                                                                                <div class="col-md-7" style="float: left">
                                                                                                    <select style="width:125px" id="editRepeatType_@details.ID-interval-selection" class="form-control" onchange="javascript:ModifyVisibility('editRepeatType_@details.ID');">
                                                                                                        <option value="1" @if (details.RepeatEveryType == "DAY(s)") { @Html.Raw("selected")   }>DAY(s)</option>
                                                                                                        <option value="2" @if (details.RepeatEveryType == "WEEK(s)") { @Html.Raw("selected")   }>WEEK(s)</option>
                                                                                                        <option value="3" @if (details.RepeatEveryType == "MONTH(s)") { @Html.Raw("selected")   }>MONTH(s)</option>
                                                                                                        <option value="4" @if (details.RepeatEveryType == "YEAR(s)") { @Html.Raw("selected")   }>YEAR(s)</option>
                                                                                                    </select>
                                                                                                </div>
                                                                                            </div>
                                                                                        </td>

                                                                                        @* ---[ ON ]---*@
                                                                                        <td>
                                                                                            <div class="scheduleDetail_@details.ID" style="width: 100px">
                                                                                                <label id="labelOn_@details.ID">
                                                                                                    @if (details.RepeatEveryType != "DAY(s)")
                                                                                                    {
                                                                                                        if (details.RepeatEveryType == "WEEK(s)")
                                                                                                        {
                                                                                                            foreach (var tmp in details.DayOfWeek)
                                                                                                            {
                                                                                                                @Html.Raw(tmp + " ")
                                                                                                            }
                                                                                                        }

                                                                                                        if (details.RepeatEveryType == "MONTH(s)")
                                                                                                        {
                                                                                                            @details.GiornoMese
                                                                                                        }

                                                                                                        if (details.RepeatEveryType == "YEAR(s)")
                                                                                                        {
                                                                                                            @details.GiornoAnno.Day;
                                                                                                            @Html.Raw("/");
                                                                                                            @details.GiornoAnno.Month;
                                                                                                        }
                                                                                                    }
                                                                                                    else
                                                                                                    {
                                                                                                        @Html.Raw("-");
                                                                                                    }
                                                                                                </label>
                                                                                            </div>
                                                                                            @* ---[ edit ]---*@
                                                                                            <div style="display:none" class="input-group scheduleDetail_@details.ID">
                                                                                                <div class="input-group" id="editRepeatType_@details.ID-Reporta">
                                                                                                    -
                                                                                                </div>
                                                                                                <div class="input-group" id="editRepeatType_@details.ID-Reportb" style="display:none;">
                                                                                                    <select id="editRepeatType_@details.ID-reportb-days-list" data-width="100px" class="form-control bs-select" multiple>
                                                                                                        <option value="1" @if (details.DayOfWeek.Contains("Mon")) { @Html.Raw("selected")  }>Mon</option>
                                                                                                        <option value="2" @if (details.DayOfWeek.Contains("Tue")) { @Html.Raw("selected")  }>Tue</option>
                                                                                                        <option value="3" @if (details.DayOfWeek.Contains("Wed")) { @Html.Raw("selected")  }>Wed</option>
                                                                                                        <option value="4" @if (details.DayOfWeek.Contains("Thu")) { @Html.Raw("selected")  }>Thu</option>
                                                                                                        <option value="5" @if (details.DayOfWeek.Contains("Fri")) { @Html.Raw("selected")  }>Fri</option>
                                                                                                        <option value="6" @if (details.DayOfWeek.Contains("Sat")) { @Html.Raw("selected")  }>Sat</option>
                                                                                                        <option value="7" @if (details.DayOfWeek.Contains("Sun")) { @Html.Raw("selected")  }>Sun</option>
                                                                                                    </select>
                                                                                                </div>
                                                                                                <div class="input-group" id="editRepeatType_@details.ID-Reportc" style="display:none">
                                                                                                    <input id="editRepeatType_@details.ID-reportc-numtday" style="width:39px; text-align:center" value="@details.GiornoMese" class="positive touchspin_days_in_month" type="text" name="number">
                                                                                                </div>
                                                                                                <div class="input-group date date-picker" data-date-format="dd/mm" data-date-start-date="+0d" id="editRepeatType_@details.ID-Reportd" style="display:none">
                                                                                                    <input id="editRepeatType_@details.ID-reportd-date" style="width:60px" value="@details.GiornoAnno.Day@Html.Raw("/")@details.GiornoAnno.Month" type="text" class="form-control" readonly>
                                                                                                    <span class="input-group-btn">
                                                                                                        <button class="btn default" type="button">
                                                                                                            <i class="fa fa-calendar"></i>
                                                                                                        </button>
                                                                                                    </span>
                                                                                                </div>
                                                                                            </div>
                                                                                        </td>

                                                                                        @* ---[ STORAGE PERIOD ]---*@
                                                                                        <td>
                                                                                            <div class="scheduleDetail_@details.ID">
                                                                                                <label id="labelStorageEveryNumb_@details.ID"> @details.StoragePeriodNumb </label>  -
                                                                                                <label id="labelStorageEveryType_@details.ID"> @details.StoragePeriodType </label>
                                                                                            </div>
                                                                                            <div style="display:none" class="input-group scheduleDetail_@details.ID">
                                                                                                <div class="col-md-7">
                                                                                                    <input id="editStorageEveryNumb_@details.ID" value="@details.StoragePeriodNumb" style="width:57px; text-align:center" class="positive touchspin_default" type="text" name="number">
                                                                                                </div>
                                                                                                <div class="col-md-7" style="float: left">
                                                                                                    <select id="editStorageEveryType_@details.ID" style="width:120px" class="form-control">
                                                                                                        <option value="DAY(s)" @if (details.StoragePeriodType == "DAY(s)") { @Html.Raw("selected") }>DAY(s)</option>
                                                                                                        <option value="WEEK(s)" @if (details.StoragePeriodType == "WEEK(s)") { @Html.Raw("selected") }>WEEK(s)</option>
                                                                                                        <option value="MONTH(s)" @if (details.StoragePeriodType == "MONTH(s)") { @Html.Raw("selected") }>MONTH(s)</option>
                                                                                                        <option value="YEAR(s)" @if (details.StoragePeriodType == "YEAR(s)") { @Html.Raw("selected") }>YEAR(s)</option>
                                                                                                    </select>
                                                                                                </div>
                                                                                            </div>
                                                                                        </td>

                                                                                        @* ---[ ACTIONS ]---*@
                                                                                        <td>
                                                                                            @*<div class="btn-group btn-group-circle">
                                                                                                <button type="button" class="btn btn-outline green btn-sm" data-toggle="modal" href="#modalEditSchedule_@details.ID"> Edit </button>
                                                                                                <button type="button" id='@details.ID-Delete' class="btn btn-outline red btn-sm" data-toggle="confirmation" data-id='@details.ID-@details.Report' data-original-title="Are you sure ?" data-placement="left" data-singleton="true" data-on-confirm="deleteSchedule">Delete</button>
                                                                                            </div>*@
                                                                                            <div class="btn-group btn-group-circle">
                                                                                                @*<button type="button" class="btn btn-outline green btn-sm" data-toggle="modal" href="#modalEditSchedule_@details.ID"> Edit </button>*@
                                                                                                <button type="button" class="btn btn-outline green btn-sm scheduleDetail_@details.ID" data-toggle="modal" id="editButtonScheduler_@details.ID" onclick="javascript: editRowClick(@details.ID, this)"> Edit </button>
                                                                                                <button type="button" class="btn btn-outline red btn-sm scheduleDetail_@details.ID" data-toggle="confirmation" id="deleteButtonScheduler_@details.ID" data-id='@details.ID-@details.Report' data-original-title="Are you sure ?" data-placement="left" data-singleton="true" data-on-confirm="deleteRowClick">Delete</button>
                                                                                            </div>
                                                                                            <div class="btn-group btn-group-circle">
                                                                                                <button type="button" style="display:none" class="btn btn-outline green btn-sm scheduleDetail_@details.ID" data-toggle="modal" id="confirmButtonScheduler_@details.ID" onclick="javascript: okRowClick(@details.ID, this, '@item.Report')"> Ok </button>
                                                                                                <button type="button" style="display:none" class="btn btn-outline red btn-sm scheduleDetail_@details.ID" data-toggle="modal" id="undoButtonScheduler_@details.ID" onclick="javascript: undoRowClick(@details.ID, this)" data-placement="left"> Undo </button>
                                                                                            </div>
                                                                                        </td>
                                                                                    </tr>
                                                                                }
                                                                            }

                                                                            @* ------------------[ INSERT NEW ROW ]------------------*@
                                                                            <tr class="add-row" id="@item.Report.Replace("'", "_").Replace(" ", "_")-scheduleAdd">

                                                                                @* ---[ OSP ]---*@
                                                                                <td>
                                                                                    <div class="input-group">
                                                                                        <select id="@item.Report.Replace(" ","_").Replace("'","-")-osps-selection" data-width="60px" class="bs-select form-control" multiple>
                                                                                            @foreach (var tmp in Model.reportsObj.ReportList[0].OSPs)
                                                                                            {
                                                                                                <option value="@Html.Raw(tmp)" selected> @Html.Raw(tmp)</option>
