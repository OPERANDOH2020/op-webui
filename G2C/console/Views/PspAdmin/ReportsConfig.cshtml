                                                        @*<td class="accordion-toggle" data-toggle="collapse" data-target="#@item.Report.Replace("'", "_").Replace(" ", "_")-schedule">*@
                                                        <td>
                                                            <i onclick="$('#@item.Report.Replace("'", "_").Replace(" ", "_")-schedule').toggle()" class="fa fa-plus"></i>
                                                        </td>
