                                                    <!--  ----------------   USERS TABLE   -------------------   -->
                                                    <!--  ----------------------------------------------------   -->
                                                    <div class="col-xs-12 col-md-12">
                                                        <div class="">
                                                            <table class="table table-striped table-bordered table-hover order-column" id="userManagement_psp">
                                                                <thead>
                                                                    <tr>
                                                                        <th class="sorting" style="width:85px;"> ID </th>
                                                                        <th class="sorting" style="width:125px;"> User name </th>
                                                                        <th class="sorting" style="width:125px;"> Type </th>
                                                                        <th class="sorting"> E-mail </th>
                                                                        <th class="sorting" style="width:105px;">Actions</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    @{ string oddeven = "odd";
                                                                        int count = 1; }
                                                                    @foreach (var user in Model)
                                                                    {
                                                                        <tr id="PSP" class="@(oddeven) gradeX">
                                                                            <td>40</td>
                                                                            <td>@user.userName</td>
                                                                            <td>@user.userType</td>
                                                                            <td>@user.email</td>
                                                                            <td>
                                                                                <div class="btn-group btn-group-circle">
                                                                                    @if (count == 1)
                                                                                    {
                                                                                        <button type="button" class="btn btn-outline green btn-sm scheduleDetail_1" onclick="location.href='@Url.Action("UsersManagementEdit", "PspAdmin")';return false;">Edit</button>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <button type="button" onclick="$('#tabUserManagement01').removeClass('active'); $('#tabUserManagement03').addClass('active');" class="btn btn-outline green btn-sm scheduleDetail_@(count)" data-toggle="tab" id="editButtonScheduler_@(count)" href="#tabUserManagement03"> Edit </button>
                                                                                    }
                                                                                    <!--
                                                                                    <button type="button" class="btn btn-outline red btn-sm scheduleDetail_@(count)" 
                                                                                            data-toggle="confirmation" id="deleteButtonScheduler_@(count)" 
                                                                                            data-id="@(count)" data-original-title="Are you sure ?" 
                                                                                            data-placement="left" data-singleton="true" 
                                                                                            data-on-confirm="deleteUser('@(user.userName)')">Delete</button>
                                                                                        -->
                                                                                    <button type="button" class="btn btn-outline red btn-sm scheduleDetail_@(count)"
                                                                                            id="delete_user_@(count)"
                                                                                            onclick="deleteUser('@(user.userName)')">Delete
                                                                                            </button>
                                                                                </div>
                                                                            </td>
                                                                        </tr>
                                                                        if (oddeven == "odd") { oddeven = "even"; }
                                                                        count++;
                                                                    }

                                                                </tbody>
                                                            </table>

                                                            <script>
                                                                function deleteUser(username) {
                                                                    var urlUserAdd = "@Url.Action("UsersManagementDelete", "PspAdmin")/panos";
                                                                    $.ajax({
                                                                        url: urlUserAdd,
                                                                        type: "put",
                                                                        datatype: 'text',
                                                                        contentType: "application/json",
                                                                        data: JSON.stringify({
                                                                            userName: username
                                                                        }),
                                                                        error: function (xhr, status) { alert(status + "XHR " + xhr.responseText);}
                                                                    }).done(function () {
                                                                        alert("The user is deleted.");
                                                                    }).fail(function () {
                                                                        alert("The user could not be deleted. Please try again.");
                                                                    });
                                                                }
                                                            </script>

