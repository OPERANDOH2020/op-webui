    function ModifyVisibility(Parent) {

        var valType = ObjectType(Parent);

        switch(valType.toLowerCase()) {
            case 'day(s)':
                {
                    $('#' + Parent + "-" + "Reporta").show();
                    $('#' + Parent + "-" + "Reportb").hide();
                    $('#' + Parent + "-" + "Reportc").hide();
                    $('#' + Parent + "-" + "Reportd").hide();
                }
                break;
            case 'week(s)':
                {
                    $('#' + Parent + "-" + "Reporta").hide();
                    $('#' + Parent + "-" + "Reportb").show();
                    $('#' + Parent + "-" + "Reportc").hide();
                    $('#' + Parent + "-" + "Reportd").hide();
                }
                break;
            case 'month(s)':
                {
                    $('#' + Parent + "-" + "Reporta").hide();
                    $('#' + Parent + "-" + "Reportb").hide();
                    $('#' + Parent + "-" + "Reportc").show();
                    $('#' + Parent + "-" + "Reportd").hide();
                }
                break;
            case 'year(s)':
                {
                    $('#' + Parent + "-" + "Reporta").hide();
                    $('#' + Parent + "-" + "Reportb").hide();
                    $('#' + Parent + "-" + "Reportc").hide();
                    $('#' + Parent + "-" + "Reportd").show();
                }
                break;
            default:
                {
                    $('#' + Parent + "-" + "Reporta").hide();
                    $('#' + Parent + "-" + "Reportb").hide();
                    $('#' + Parent + "-" + "Reportc").hide();
                    $('#' + Parent + "-" + "Reportd").hide();
                }
                break;
        }




    }

    function ObjectType(Parent) {

        var Type = $('#' + Parent + "-" + "interval-selection").val();

        var selectedValintervalselection = [];
        $('#' + Parent + '-' + 'interval-selection :selected').each(function (i, selected) {
            selectedValintervalselection[i] = $(selected).text();
        });

        var valType = "";

        for (var i = 0; i < selectedValintervalselection.length; i++) {
            valType += "," + selectedValintervalselection[i];
        }

        valType = valType.replace(",", "");

        return valType.toLowerCase();

    }

    
