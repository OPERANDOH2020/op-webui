@model Operando_AdministrationConsole.Models.ReportManager
@{
    ViewBag.MenuSection = "PSP Admin";
    ViewBag.Title = "Reports (configuration)";
    Layout = "~/Views/Shared/_LayoutOperando.cshtml";
}
<script type="text/javascript">

    function salvaReport(valID)
    {
        if (valID == null) {
            var valReport = $('#Report_add').val()
            var valDescription = $('#Description_add').val()
            var valVersion = $('#Version_add').val()
            var valLocation = $('#Location_add').val()
            var valParameters = $('#Parameters_add').val()
            var selectedVal = [];
            $('#OSPs_add :selected').each(function (i, selected) {
                selectedVal[i] = $(selected).text();
                //alert(selectedVal[i]);
            });

            var valOSPs = "";

            for (var i = 0; i < selectedVal.length; i++) {
                valOSPs += "," + selectedVal[i];
            }

            $.ajax({
                async: false,
                type: "POST",
                url: "../ASPXSaveAjax/SaveReport.aspx",
                data: { ID: 0, Report: valReport, Description: valDescription, Version: valVersion, Location: valLocation, Parameters: valParameters, OSPs: valOSPs },
                dataType: "text",
                success: function (data) {

                    //var risposta = JSON.parse(data);
                },
                error: function (xhr, status) {
                    $('#button_add').removeAttr("data-title");
                    $('#button_add').removeAttr("data-message");
                    $('#button_add').removeAttr("data-type");
                    $('#button_add').removeAttr("data-confirm-button-class");
                    $('#button_add').attr("data-title", "Error");
                    $('#button_add').attr("data-message", "Error Message");
                    $('#button_add').attr("data-type", "error");
                    $('#button_add').attr("data-confirm-button-class", "btn-error");
                    reloadSweetAlert($('#button_add'));
                }
            });

        }
        else {
            var valReport = $('#Report_' + valID).val()
            var valDescription = $('#Description_' + valID).val()
            var valVersion = $('#Version_' + valID).val()
            var valLocation = $('#Location_' + valID).val()
            var valParameters = $('#Parameters_' + valID).val()


            var selectedVal = [];
            $('#OSPs_' + valID + ' :selected').each(function (i, selected) {
                selectedVal[i] = $(selected).text();
                //alert(selectedVal[i]);
            });

            var valOSPs = "";

            for (var i = 0; i < selectedVal.length; i++) {
                valOSPs += "," + selectedVal[i];
            }


            $.ajax({
                async: false,
                type: "POST",
                url: "../ASPXSaveAjax/SaveReport.aspx",
                data: { ID: valID, Report: valReport, Description: valDescription, Version: valVersion, Location: valLocation, Parameters: valParameters, OSPs: valOSPs },
                dataType: "text",
                success: function (data) {

                    //var risposta = JSON.parse(data);
                },
                error: function (xhr, status) {
                    $('#button_' + valID).removeAttr("data-title");
                    $('#button_' + valID).removeAttr("data-message");
                    $('#button_' + valID).removeAttr("data-type");
                    $('#button_' + valID).removeAttr("data-confirm-button-class");
                    $('#button_' + valID).attr("data-title", "Error");
                    $('#button_' + valID).attr("data-message", "Error Message");
                    $('#button_' + valID).attr("data-type", "error");
                    $('#button_' + valID).attr("data-confirm-button-class", "btn-error");
                    reloadSweetAlert($('#button_' + valID));
                }
            });
        }
    }

