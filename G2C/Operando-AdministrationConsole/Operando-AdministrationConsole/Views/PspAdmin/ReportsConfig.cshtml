    function deleteSchedule() {
        var valID = $(this).attr("data-id");
        $.ajax({
            async: false,
            type: "POST",
            url: "../ASPXSaveAjax/DeleteSchedule.aspx",
            data: { ID: valID },
            dataType: "text",
            success: function (data) {

                //var risposta = JSON.parse(data);
                window.location.reload();
            },
            error: function (xhr, status) {
                window.location.reload();
            }
