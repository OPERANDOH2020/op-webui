</script>


<!-- BEGIN PAGE BASE CONTENT -->
<div class="row">
    <div class="col-xs-12">
        <div class="m-heading-1 border-grey-silver m-bordered">
            <p>
                Page description here.
            </p>
        </div>

        <!-- BEGIN PORTLET -->
        <div class="portlet light bordered">
            <div class="portlet-body">
                <div class="tabbable-custom tabbable-custom-operando-top">
                    <ul class="nav nav-tabs border-yellow-gold">
                        <li id="tab_reports" class="active">
                            <a href="#tab_1_1" data-toggle="tab"> Reports </a>
                        </li>
                        <li>
                            <a href="#tab_1_2" data-toggle="tab"> Results </a>
                        </li>
                        <li id="tab_schedule">
                            <a href="#tab_1_3" data-toggle="tab"> Schedules </a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <!-- BEGIN TAB 1 (JOBS) CONTENT -->
                        <div class="tab-pane active" id="tab_1_1">
                            <div class="portlet light">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <span class="caption-subject font-dark bold uppercase">Reports list</span>
                                    </div>
                                    <div class="actions">
                                        <button type="button" class="btn btn-circle yellow-gold" data-toggle="modal" href="#modalAddJob"> Add <i class="fa fa-plus"></i></button>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <div class="row">

                                        <div class="form-group col-md-6 col-md-offset-6" style="padding-right: 0;">
                                            <form class="form-horizontal">
                                                <label class="col-md-4 control-label">Filter by OSP</label>
                                                <div class="col-md-8" style="padding-right: 0;">
                                                    <select class="form-control bs-select" multiple>
                                                        @foreach (var tmp in Model.reportsObj.ReportList[0].OSPsOption)
                                                        {
                                                            @Html.Raw(tmp)
                                                        }
                                                    </select>
                                                </div>

                                            </form>
                                        </div>

                                    </div>

                                    <div class="row">
                                        <table class="table table-striped table-bordered table-hover order-column" id="reports_psp">
                                            <thead>
                                                <tr>
                                                    <th> Report </th>
                                                    <th> Description </th>
                                                    <th> Version </th>
                                                    <th> Location </th>
                                                    <th> Parameters </th>
                                                    <th> OSPs </th>
                                                    <th> Results </th>
                                                    <th> Schedules </th>
                                                    <th> Actions </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.reportsObj.ReportList)
                                                {

                                                    <tr class="odd gradeX">
                                                        <td>@item.Report</td>
                                                        <td>@item.Description</td>
                                                        <td>@item.Version</td>
                                                        <td>@item.Location</td>
                                                        <td>@item.Parameters</td>
                                                        <td>@foreach (var tmp in item.OSPs)
                                                        {
                                                            @Html.Raw(tmp + "-")
                                                        }
                                                        </td>
                                                        <th> <a target="_blank" href="@item.Location?@item.Parameters" class="btn btn-circle btn-sm yellow-gold"> View </a> </th>
                                                        <th> <a class="btn btn-circle btn-sm yellow-gold" data-toggle="tab" href="#tab_1_3" onclick="javascript: $('#tab_reports').removeAttr('class'); $('#tab_schedule').attr('class', 'active');"> Schedules </a> </th>
                                                        <th> <a class="btn btn-circle btn-sm yellow-gold" data-toggle="modal" href="#modalEditJob_@item.ID"> Edit </a> </th>
                                                    </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END TAB 1 (REPORTS) CONTENT -->
                        <!-- BEGIN TAB 2 (RESULTS) CONTENT -->
                        <div class="tab-pane" id="tab_1_2">
                            <div class="portlet light">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <span class="caption-subject font-dark bold uppercase">Results</span>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <div class="row">

                                        <div class="form-group col-md-6 col-md-offset-6" style="padding-right: 0;">
                                            <form class="form-horizontal">
                                                <label class="col-md-4 control-label">Filter by OSP</label>
                                                <div class="col-md-8" style="padding-right: 0;">
                                                    <select class="form-control bs-select" multiple>
                                                        @foreach (var tmp in Model.reportsObj.ReportList[0].OSPsOption)
                                                        {
                                                            @Html.Raw(tmp)
                                                        }
                                                    </select>
                                                </div>

                                            </form>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <table class="table table-striped table-bordered table-hover order-column" id="reports_results_psp">
                                            <thead>
                                                <tr>
                                                    <th> Execution date</th>
                                                    <th> Report </th>
                                                    <th> Version </th>
                                                    <th> OSP </th>
                                                    <th> Actions </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.resultsObj.ResultList)
                                                {

                                                    <tr class="odd gradeX">
                                                        <td>@item.ExecutionDate</td>
                                                        <td>@item.Report</td>
                                                        <td>@item.ReportVersion</td>
                                                        <td>
                                                            @foreach (var tmp in item.OSPs)
                                                            {
                                                                @Html.Raw(tmp + "-")
                                                            }
                                                        </td>
                                                        <td>
                                                            <a target="_blank" href="../../@item.FileName" class="btn btn-circle btn-sm yellow-gold"> View </a>
                                                        </td>
                                                </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END TAB 2 (RESULTS) CONTENT -->
                        <!-- BEGIN TAB 3 (SCHEDULES) CONTENT -->
                        <div class="tab-pane" id="tab_1_3">
                            <div class="portlet light">
                                <div class="portlet-title">
                                    <div class="caption">
                                        <span class="caption-subject font-dark bold uppercase">Scheduler</span>
                                    </div>
                                    <div class="actions">
                                        <button type="button" class="btn btn-circle yellow-gold" data-toggle="modal" href="#modalAddSchedule"> Add <i class="fa fa-plus"></i></button>
                                    </div>
                                </div>
                                <div class="portlet-body">
                                    <div class="row">

                                        <div class="form-group col-md-6" style="padding-left: 0;">
                                            <form class="form-horizontal">
                                                <label class="control-label" style="float: left; width: auto;text-align: left;padding-left: 0;">Filter by Report</label>
                                                <div class="col-md-8" style="padding-right: 0;">
                                                    <select id="report_scheduleList" class="form-control bs-select" multiple>
                                                    @foreach (var item in Model.reportsObj.ReportList)
                                                    {
                                                        <option>@item.Report</option>
                                                    }
                                                    </select>
                                                </div>

                                            </form>
                                        </div>
                                        <div class="form-group col-md-6" style="padding-right: 0;">
                                            <form class="form-horizontal">
                                                <label class="col-md-4 control-label">Filter by OSP</label>
                                                <div class="col-md-8" style="padding-right: 0;">
                                                    <select class="form-control bs-select" multiple>
                                                        @foreach (var tmp in Model.reportsObj.ReportList[0].OSPsOption)
                                                        {
                                                            @Html.Raw(tmp)
                                                        }
                                                    </select>
                                                </div>

                                            </form>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <table class="table table-striped table-bordered table-hover order-column">
                                            <thead>
                                                <tr>
                                                    <th>&nbsp;</th>
                                                    <th>Report</th>
                                                    <th style="min-width: 90px;">Actions</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in Model.schedulesObj.ScheduleList)
                                                {

                                                    <tr class="odd gradeX">
                                                        <td class="accordion-toggle" data-toggle="collapse" data-target="#@item.Report-schedule">
                                                            <i class="fa fa-plus"></i>
                                                        </td>
                                                        <td>@item.Report</td>
                                                        <td>
                                                            <div class="btn-group">
                                                                <button type="button" class="btn btn-outline yellow-gold btn-sm" data-toggle="collapse" data-target="#@item.Report.Replace("'","_")-schedule">Details</button>
                                                            </div>
                                                        </td>
                                                    </tr>

                                                    <tr class="active accordion-body collapse" id="@item.Report.Replace("'", "_")-schedule">
                                                    <td colspan="8">
                                                        <div class="portlet light">
                                                            <div class="portlet-title">
                                                                <div class="caption">
                                                                    <span class="caption-subject font-dark bold uppercase">Schedule</span>
                                                                </div>
                                                                <div class="actions">
                                                                    
                                                                    <button type="button" class="btn btn-circle yellow-gold" data-toggle="modal" href="#modalAddScheduleDet_@item.Report.Replace("'", "_")"> Add <i class="fa fa-plus"></i></button>
                                                                </div>
                                                            </div>
                                                            <div class="portlet-body">
                                                                <table class="table table-bordered table-striped">
                                                                    <thead>
                                                                        <tr>
                                                                            <th>OSP</th>
                                                                            <th>Start Date</th>
                                                                            <th>Run every</th>
                                                                            <th>On</th>
                                                                            <th>Storage period</th>
                                                                            <th style="width: 125px;">Actions</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        @foreach (var details in Model.schedulesObj.ScheduleDetailsList)
                                                                        {
                                                                            if (details.Report == item.Report)
                                                                            { 
                                                                                <tr>
                                                                                    <td>
                                                                                        @foreach (var tmp in details.OSPs)
                                                                                        {
                                                                                            @Html.Raw(tmp + "-")
                                                                                        }
                                                                                    </td>
                                                                                    <td>@details.StartDate</td>
                                                                                    <td>
                                                                                        @details.RepeatEveryNumb -&nbsp; @details.RepeatEveryType
                                                                                    </td>
                                                                                    <td>
                                                                                        @foreach (var tmp in details.DayOfWeek)
                                                                                        {
                                                                                            @Html.Raw(tmp + "-")
                                                                                        }
                                                                                    </td>
                                                                                    <td>
                                                                                        @details.StoragePeriodNumb -&nbsp; @details.StoragePeriodType
                                                                                
                                                                                    </td>
                                                                                    <td>
                                                                                        <div class="btn-group btn-group-circle">
                                                                                            <!-- <button type="button" class="btn btn-outline green btn-sm">Edit</button>-->
                                                                                            <button type="button" class="btn btn-outline green btn-sm" data-toggle="modal" href="#modalEditSchedule_@details.ID"> Edit </button>
                                                                                            <button type="button" class="btn btn-outline red btn-sm" data-toggle="confirmation" data-id='@details.ID' data-original-title="Are you sure ?" data-placement="left" data-singleton="true" data-on-confirm="deleteSchedule">Delete</button>
                                                                                        </div>
                                                                                    </td>
                                                                                </tr>
                                                                            }
                                                                        }
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END TAB 3 (SCHEDULES) CONTENT -->
                    </div>
                </div>
            </div>
        </div>
        <!-- END PORTLET -->
    </div>
</div>
