<!-- /.modal Add Job -->
<div id="modalAddJob" class="modal fade" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title">Add Report</h4>
            </div>
            <div class="modal-body">
                <div class="scroller" style="height:400px" data-always-visible="1" data-rail-visible1="1">
                    <div class="row">
                        <div class="col-md-12">
                            <form class="form-horizontal" role="form">
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Report</label>
                                    <div class="col-md-9">
                                        <input type="text" id="Report_add" class="form-control" placeholder="Enter report name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Description</label>
                                    <div class="col-md-9">
                                        <input type="text" id="Description_add" class="form-control" placeholder="Enter report description">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Version</label>
                                    <div class="col-md-9">
                                        <input type="text" id="Version_add" class="form-control" placeholder="Enter report Version">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Location</label>
                                    <div class="col-md-9">
                                        <input type="text" id="Location_add" class="form-control" placeholder="Enter Location URL">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Parameters</label>
                                    <div class="col-md-9">
                                        <input type="text" id="Parameters_add" class="form-control" placeholder="Enter parameters list">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">OSPs</label>
                                    <div class="col-md-9">
                                        <select id="OSPs_add" class="form-control bs-select" multiple>
                                            @foreach (var tmp in Model.reportsObj.ReportList[0].OSPsOption)
                                            {
                                                @Html.Raw(tmp)
                                            }
