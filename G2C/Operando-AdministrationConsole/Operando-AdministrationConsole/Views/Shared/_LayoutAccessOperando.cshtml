<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Operando G2C</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="@ViewBag.Title - Operando G2C" name="description" />
    <meta content="Operando Consortium" name="author" />

    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    @Styles.Render("~/operando/global-mandatory/css")
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGINS STYLES  -->
    @RenderSection("PagePluginStyles", required: false)
    <!-- END PAGE LEVEL PLUGINS STYLES -->
    <!-- BEGIN THEME GLOBAL STYLES -->
    @Styles.Render("~/operando/global-theme/css")
    <!-- END THEME GLOBAL STYLES -->
    <!-- BEGIN PAGE LEVEL STYLES -->
    @RenderSection("PageStyles", required: false)
    <!-- END PAGE LEVEL STYLES -->
    <!-- BEGIN THEME LAYOUT STYLES -->
    @Styles.Render("~/operando/global-layout/css")
    <!-- END THEME LAYOUT STYLES -->
</head>
<!-- END HEAD -->
<body class="@ViewBag.BodyCssClass">
    @RenderBody()

    <!--[if lt IE 9]>
    @Scripts.Render("~/operando/plugins/old-ie")
    <![endif]-->
    <!-- BEGIN CORE PLUGINS -->
    @Scripts.Render("~/operando/plugins/core")
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    @RenderSection("PagePlugins", required: false)
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    @Scripts.Render("~/operando/scripts/global-theme")
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    @RenderSection("PageScripts", required: false)
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    @Scripts.Render("~/operando/scripts/layout-theme")
    <!-- END THEME LAYOUT SCRIPTS -->

</body>
</html>
